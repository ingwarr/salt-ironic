        engine: rabbitmq
        host: localhost
        port: 5672
        user: openstack
        password: password
        virtual_host: '/openstack'
      dhcp_provider: none
      ipxe_enabled: true
      automated_clean: 'false'
      webserver_use: true
      http_boot_folder: /httpboot
      enable_cors: false
      tftp:
        host: 0.0.0.0
        root_directory: /tftpboot
      drivers:
        - engine: pxe_ipmitool
        - engine: agent_ipmitool
