  ironic:
    server:
      enabled: true
      version: mitaka
      testing: false
#      extra_kernel_options: ''
      deployment_address: 10.0.175.2
      file_url_port: 8080
      database:
        engine: mysql
        host: localhost
        port: 3306
        name: ironic
        user: ironic
        password: password
      enable_keystone: false
      identity:
        engine: keystone
        region: RegionOne
        host: localhost
        port: 35357
        user: ironic
        password: password
        tenant: service
      bind:
        address: 0.0.0.0
        port: 6385
